{
    "name": "react",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev-package": "npx yalc add @locked-dobby/library-react-dialog && cd ../.. && npx tsc-watch --onSuccess 'npx yalc push --sig'",
        "install-package": "rm -rf node_modules && CURRENT=$PWD && cd ../.. && TARGET=$(npm run build-local | tail -n 1) && echo install... $TARGET && cd $CURRENT && npm install ../../$TARGET",
        "build": "tsc -b && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview"
    },
    "dependencies": {
        "@locked-dobby/library-react-dialog": "file:../../package-locked-dobby-library-react-dialog-1.0.3.tgz",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.25.1"
    },
    "devDependencies": {
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^7.15.0",
        "@typescript-eslint/parser": "^7.15.0",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "eslint": "^8.57.0",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.7",
        "typescript": "^5.2.2",
        "vite": "^5.3.4",
        "vite-plugin-restart": "^0.4.1"
    }
}
